    },
    "setupFiles": [
      "<rootDir>/__mocks__/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/bocombbm-mf-ib.js",
      "!src/components/**/styles.js",
      "!src/components/**/index.js",
      "!src/components/**/*.stories.js",
      "!src/styles/**/*.js",
      "!src/components/Routes/Routes.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/shim.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
